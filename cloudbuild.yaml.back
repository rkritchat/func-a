steps:
  - name: "gcr.io/google.com/cloudsdktool/cloud-sdk"
    args:
      - gcloud
      - functions
      - deploy
      - go-funcv2
      - --entry-point=HelloHTTP
      - --gen2
      - --region=australia-southeast1
      - --source=.
      - --trigger-http
      - --runtime=go121
      - --no-allow-unauthenticated
      - --run-service-account=cloudFuncV2@totemic-web-426910-r5.iam.gserviceaccount.com

